<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>consume</servlet-name>
        <servlet-class>Dao.consume</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>consume</servlet-name>
        <url-pattern>/consume</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>profit_del</servlet-name>
        <servlet-class>Dao.profit_del</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>profit_del</servlet-name>
        <url-pattern>/profit_del</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>profit_modify</servlet-name>
        <servlet-class>Dao.profit_modify</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>profit_modify</servlet-name>
        <url-pattern>/profit_modify</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>manage_del</servlet-name>
        <servlet-class>Dao.manage_del</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>manage_del</servlet-name>
        <url-pattern>/manage_del</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>manage_add</servlet-name>
        <servlet-class>Dao.manage_add</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>manage_add</servlet-name>
        <url-pattern>/manage_add</url-pattern>
    </servlet-mapping>

    <servlet>
    <servlet-name>set_cost</servlet-name>
    <servlet-class>Dao.set_cost</servlet-class>
</servlet>
    <servlet-mapping>
        <servlet-name>set_cost</servlet-name>
        <url-pattern>/set_cost</url-pattern>
    </servlet-mapping>


</web-app>

